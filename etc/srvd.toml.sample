src = "/etc/haproxy/haproxy.cfg.tmpl"
dest = "/etc/haproxy/haproxy.cfg"
domain = "_mysql._tcp.example.com"
reload_cmd = "/usr/sbin/haproxy -c -V -f {{ .src }}"
check_cmd = "/bin/systemctl reload haproxy.service"
interval = 1
timeout = 3
